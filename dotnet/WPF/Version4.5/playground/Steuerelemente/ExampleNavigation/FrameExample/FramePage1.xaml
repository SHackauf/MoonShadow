<Page x:Class="ExampleNavigation.FrameExample.FramePage1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" d:DesignHeight="100" d:DesignWidth="120"
      Title="FramePage1">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Label Grid.Row="0">Page 1</Label>
        <TextBlock Grid.Row="1">
            <Hyperlink NavigateUri="FramePage2.xaml">Navigate to Page 2</Hyperlink>
        </TextBlock>
    </Grid>
</Page>

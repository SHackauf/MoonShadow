<Window x:Class="ExampleWindow.WindowDarstellung.DarstellungWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DarstellungWindow" Height="300" Width="300"
        ResizeMode="NoResize"
        WindowStyle="ToolWindow">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Label Grid.Row="0" Name="ResizeModeLabel"></Label>
        <Label Grid.Row="1" Name="WindowStyleLabel"></Label>
    </Grid>
</Window>
